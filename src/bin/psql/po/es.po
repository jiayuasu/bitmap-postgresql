msgid "define a new function"
msgstr "define una nueva función"

#: sql_help.h:186
msgid ""
"CREATE [ OR REPLACE ] FUNCTION\n"
"    name ( [ [ argmode ] [ argname ] argtype [, ...] ] )\n"
"    [ RETURNS rettype ]\n"
"  { LANGUAGE langname\n"
