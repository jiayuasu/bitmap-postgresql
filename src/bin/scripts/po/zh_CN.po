#: createuser.c:306
#, c-format
msgid ""
"%s creates a new PostgreSQL role.\n"
"\n"
msgstr ""
"%s 创建一个新的 PostgreSQL 用户.\n"
"\n"

#: createuser.c:308 dropuser.c:140
#, c-format
msgid "  %s [OPTION]... [ROLENAME]\n"
msgstr "  %s [选项]... [用户名]\n"

#: createuser.c:310
#, c-format
msgid "  -s, --superuser           role will be superuser\n"
msgstr "  -s, --superuser          角色将是超级用户\n"

