"    FROM { [ GROUP ] rolename | PUBLIC } [, ...]\n"
"    [ CASCADE | RESTRICT ]\n"
"\n"
"REVOKE [ GRANT OPTION FOR ]\n"
"    { { CREATE | USAGE } [,...] | ALL [ PRIVILEGES ] }\n"
"    ON SCHEMA schemaname [, ...]\n"
"    FROM { [ GROUP ] rolename | PUBLIC } [, ...]\n"
"    [ CASCADE | RESTRICT ]\n"
"\n"
"REVOKE [ GRANT OPTION FOR ]\n"
"    { CREATE | ALL [ PRIVILEGES ] }\n"
"    ON TABLESPACE tablespacename [, ...]\n"
"    FROM { [ GROUP ] rolename | PUBLIC } [, ...]\n"
